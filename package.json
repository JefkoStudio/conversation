{
  "author": "Daniel Jeffery",
  "dependencies": {
    "commander": "^11.1.0",
    "mermaid": "^10.5.0"
  },
  "description": "Flow graph oriented form navigation, generated from a UML flowchart.",
  "devDependencies": {
    "@semantic-release/git": "^10.0.1",
    "@types/jest": "^29.5.6",
    "@types/node": "^20.8.6",
    "esbuild": "^0.19.5",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^39.1.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.2.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "prettier-plugin-jsdoc": "^1.1.1",
    "semantic-release": "^22.0.6",
    "ts-alias": "^0.0.7",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "typescript-eslint": "^0.0.1-alpha.0"
  },
  "directories": {
    "bin": "bin"
  },
  "files": [
    "dist/**/*",
    "docs"
  ],
  "keywords": [
    "Jefko",
    "form",
    "conversation",
    "uml",
    "flow",
    "flowchart",
    "graph"
  ],
  "license": "MIT",
  "module": "dist/index.js",
  "name": "@jefko/conversation",
  "overrides": {
    "mermaid": {
      "dompurify": {
        ".": "npm:isomorphic-dompurify",
        "dompurify": "^3.0.6"
      }
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/JefkoStudio/conversation.git"
  },
  "scripts": {
    "precompile": "rm -rf dist",
    "compile": "npx tsc --skipLibCheck",
    "postcompile": "npx tsc-alias",
    "lint": "eslint . --ext .ts",
    "prepare": "npm run compile -- --declarationMap false --sourceMap false",
    "pretest": "npm run lint",
    "test": "NODE_OPTIONS=--experimental-vm-modules npx jest"
  },
  "type": "module",
  "types": "dist/index.d.ts",
  "version": "1.0.1"
}
